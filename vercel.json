{
  "version": 2,
  "name": "ai-safety-zones-app",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/client/$1" }
  ],
  "framework": "vite",
  "env": {
    "VITE_ENABLE_GOOGLE_MAPS": "true"
  }
}
